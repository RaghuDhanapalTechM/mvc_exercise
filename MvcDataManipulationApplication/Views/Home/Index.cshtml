@{
    ViewBag.Title = "Home Page";
}

<style type="text/css">
    table tr td {
        border: 1px solid black
    }

    table tr th {
        border: 2px solid black
    }
</style>

<div class="row">
    <br />
    <table style="border:1px solid black ">

        <tr>
            <th>
                ID
            </th>
            <th>
                FName
            </th>
            <th>
                LName
            </th>
            <th>
                Age
            </th>
            <th>
                Dept
            </th>
        </tr>
        <tr>

            @foreach (var o in ViewBag.DataDispaly as List<MvcDataManipulationApplication.Models.MyData>)
            {
            <tr>
                <td>
                    @o.Id
                </td>
                <td>
                    @o.FName
                </td>
                <td>
                    @o.LName
                </td>
                <td>
                    @o.Age
                </td>
                <td>
                    @o.Dept
                </td>
                <td> <a href="home/contact/@o.Id">Edit</a></td>
            </tr>
            }




        </table>
    </div>